<?xml version="1.0" encoding="UTF-8"?>
<routings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/routingconfig.xsd">
    <routing name="nonPo">
		<queue name="Approve" inbox="true" enforceApprovalLimit="false"/>
        <displayProperties>
            <property name="site"/>
            <property name="displayName"/>
            <property name="email"/>
            <property name="approvalLimit"/>
        </displayProperties>
    </routing>
    <routing name="po">
        <queue name="Approve" inbox="false" enforceApprovalLimit="true"/>
        <displayProperties>
            <property name="displayName"/>
            <property name="email"/>
            <property name="approvalLimit"/>
            <property name="paymentOveride"/>
        </displayProperties>
    </routing>
	<routing name="MF">
        <queue name="Approve" inbox="true" enforceApprovalLimit="true"/>
        <displayProperties>
            <property name="displayName"/>
            <property name="email"/>
        </displayProperties>
    </routing>
    <routing name="CR-MISC">
        <queue name="Approve" inbox="true" enforceApprovalLimit="true"/>
        <displayProperties>
            <property name="displayName"/>
            <property name="email"/>
            <property name="approvalLimit"/>
            <property name="employeeNumber"/>
        </displayProperties>
    </routing>
    <routing name="CR-OCEAN">
            <queue name="Approve" inbox="true" enforceApprovalLimit="false"/>
            <displayProperties>
                <property name="displayName"/>
                <property name="email"/>
                <property name="employeeNumber"/>
            </displayProperties>
    </routing>
</routings>

