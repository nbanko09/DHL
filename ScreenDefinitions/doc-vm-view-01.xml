<?xml version="1.0" encoding="UTF-8"?>
<view name="doc-vm-view-01" template="doc-tax-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd">

	<labels default="en">
		<locale name="en">
		</locale>
	</labels>

	<section-overrides>	
	</section-overrides>
	
		
	<property-overrides>
		<textArea name="processInformation.comment" readonly="false"  />
		<dropDown name="subType" readonly="true" />
		<dropDown name="sensitivity" readonly="true" />
		<date name="date" readonly="true" />
		<checkbox name="tbdBoolean1" readonly="true" />
		<date name="tbdDate1" readonly="true"/>
		<dropDown name="tbdString1" readonly="true" styleClass="onefifty" />
		<date name="tbdDate2" readonly="true" />
		<textArea name="voucherComments" readonly="true" />
		<text name="vendor.name" validation="" readonly="true"/>		
		<lookup name="vendor.name" readonly="false" />
	</property-overrides>

	<lineItem-overrides> 
	</lineItem-overrides>

	<action-overrides>
			<getNext name="next" readonly="true" visible="false" />
			<rejectButton name="reject" readonly="false"  />
			<button name="update" readonly="true" />
			<button name="forward" readonly="false" />
	</action-overrides>

	<hidden-overrides>
		<hidden name="processInformation.response" defaultValue="Complete"/>
	</hidden-overrides>

	 <documentRejection>
		<rejectionReasons>
			<reason name="Reject" commentsRequired="true" note="message.returnException">
				<subReason name="Duplicate Tax Form"/>
				<subReason name="Incomplete documentation"/>
				<subReason name="Not a Genuine Tax Form"/>
				<subReason name="Other"/>
			</reason>
		</rejectionReasons>
	</documentRejection>

	<validations>
		<validation name="validation.validVendor" messageType="warning" note="message.validVendorValidation"/>
	</validations>
</view>
