<?xml version="1.0" encoding="UTF-8"?>
<propertyTemplate
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/template.xsd">

	<labels default="en">
		<locale name="en">
		<label name="forward" value="Submit"/>
		<label name="group" value="Control Station"/>
		</locale>
	</labels>

		
		<section name="doc" collapsible="false" collapsed="false">
			<properties>
			<textArea row="1" col="1-2" name="voucherComments" readonly="true" styleClass="comment"/> 
			<textArea row="2" col="1-2" name="tbdComment1" styleClass="comment"/> 
			<output row="3" col="1-2" name="processInformation.previousComment" readonly="true" styleClass="comment" escapeXml="false"/>
			<valueAlias row="4" col="1" name="processInformation.queueName" readonly="true"/>		
			</properties>
		<actions>
			<button name="forward" command="forward" readonly="false" docType="upload" type="submit" isAnchored="false" title="forwardTitle" styleClass="button70"/>
		</actions>			
		</section>		

	<section name="requestCustomer" visible="true" collapsible="false" collapsed="false">
		<properties>
			<dropDown row="1" col="1" name="processCode" readonly="false" validation="required" choiceList="choice.product" onChange="reloadPage(this)"/>/> <!--Product-->
			<dropDown row="1" col="2" name="subType" choiceList="choice.masterfileCustomerBrkSubTypes" readonly="false" validation="required" onChange="reloadPage(this)"/> <!--Request Type-->
			<dropDown row="1" col="3" name="department" choiceList="choice.updateAreaDom" validation="required" onChange="reloadPage(this)"/> <!--Area to Be Updated-->
			<dropDown row="2" col="1" name="docPriority" choiceList="choice.priority" defaultValue="1" readonly="false" validation="required"/> <!--Priority-->
			<text row="2" col="2" name="custom4.contactNo" validation="required, noSpecialCharacters"/> <!--Contact No-->
			<dynamicDropDown row="2" col="3" name="group" readonly="false" providerClass="com.miriasystems.ui.providers.GroupsChoiceListProvider" validation="required"/> <!--Control Station-->
		</properties>
		<actions>
			<iconAction name="print" icon="/images/ico-print-w24.png" width="24" height="24" link="javascript:window.print()" visible="true" alwaysActive="true"/>
			<iconAction name="statusWindow" icon="/images/ico-history-w24.png" width="24" height="24" link="javascript:openStatusWindow('$seqId$', '$contextPath$');return false;" alwaysActive="true"/>
			<iconAction name="help" icon="/images/ico-help-w24.png" width="24" height="24" link="javascript:openHelp('User_Interfaces/Working_with_Invoices.htm', '$contextPath$')" alwaysActive="true"/>
		</actions>
		</section>
		
	<section name="customerDetails" visible="true" collapsible="false" collapsed="false">
			<properties>
			<text row ="1" col="1" name="custom4.accountNumber" readonly="false" validation="required"/> <!--Account Code-->
			<text row="2" col="1-3" name="custom4.name" readonly="false" validation="required" styleClass="fulllookup"/> <!--Account Name-->
			</properties>
		</section>
	
		<section name="billTo" visible="true" collapsible="false" collapsed="false">
			<properties>
				<checkbox row="1" col="1" name="tbdBoolean2" readonly="false"/> <!--As Above-->
				<text row="2" col="1" name="custom4.billToAccountCode" readonly="false" styleClass="fulllookup"/> <!--Alt Bill To Ac-->
				<text row="2" col="2-5" name="custom4.billToAccount" readonly="false" styleClass="fulllookup"/> <!--Alt Bill To Name-->
			</properties>
		</section>
		
		<section name="shipperInfo" visible="true" collapsible="false" collapsed="false">
			<properties>
				<checkbox row="1" col="1" name="custom4.knownShipper" readonly="false"/> <!--Known Shipper-->
				<checkbox row="1" col="2" name="custom4.poaReceived" readonly="false"/> <!--POA Received-->
				<checkbox row="1" col="3" name="custom4.untilRevoked" readonly="false"/> <!--Until Revoked-->
				<date row="2" col="1" name="custom4.poaExpiryDate" readonly="false"/> <!--POA Expiry Date-->
				<text row="2" col="2" name="custom4.poaLocation" readonly="false" /> <!--POA Location-->
				<dropDown row="2" col="3" name="custom4.poaFl" choiceList="choice.poaFL" readonly="false" /> <!--POA F/L-->
				<checkbox row="3" col="1" name="custom4.option4" readonly="false"/> <!--Option 4-->
				<checkbox row="3" col="2" name="custom4.selfFilling" readonly="false"/> <!--Self Filling-->
		    </properties>
		</section>
	
		<section name="additionalInformation" visible="true" collapsible="false" collapsed="false">
			<properties>
				<text row="1" col="1" name="custom4.contactName" readonly="false"/>  <!--Contact Name-->
				<text row="1" col="2" name="custom4.contactPhone" readonly="false"/> <!--Contact Tel #-->
				<text row="1" col="3" name="custom4.salesRep" readonly="false"/>    <!--Sales Rep-->
				<checkbox row="2" col="1" name="custom4.contractInEffect" readonly="false"/> <!--Contract In Effect-->
				<text row="2" col="2" name="custom4.contractNo" readonly="false"/>   <!--Contract Number-->
				<text row="3" col="1" name="custom4.linkToCorpContract" readonly="false"/>   <!--Link to Corp COntract-->
				<text row="3" col="2" name="custom4.linkToCorpAccount" readonly="false"/>   <!--Link to Corp Acc-->
				<text row="4" col="1" name="custom4.brokerName" readonly="false"/>  <!--Broker-->
				<text row="4" col="2" name="custom4.brokerAccount" readonly="false"/>  <!--Broker Ac-->				
			</properties>
		</section>

	<hiddenFields>
	<hidden name="requesterId"/>
	<hidden name="scanVendorName"/>
	<hidden name="type" defaultValue="CUSTOMER"/>
	<hidden name="uploadRef"/>
	<hidden name="originatorId"/>
	<hidden name="retentionBoxId" defaultValue="upload" forceDefault="true"/>
    </hiddenFields>
	
	<processors>
	</processors>
</propertyTemplate>
