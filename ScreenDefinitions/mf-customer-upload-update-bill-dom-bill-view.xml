<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="masterfile-upload-customer-update-dom-tpl.xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en">
        </locale>
    </labels>
    
    <section-overrides>
             <section name="additionalInformation" visible="false" />
    </section-overrides>
    
    <property-overrides>
       <textArea name="voucherComments" defaultValue="Warning! Request to update an account must be approved. Please forward to a local authorized Approver or attach supporting documentation."/>
       <textArea name="tbdComment1" validation="required"/>
       <text name="custom4.billToAccountCode" readonly="true"/> <!--Alt Bill To Ac-->
	<text name="custom4.billToAccount" readonly="true"/> <!--Alt Bill To Name-->
    </property-overrides>

    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>

    <validations>   
    </validations>
</view>
