<?xml version="1.0" encoding="UTF-8"?>
<propertyTemplate
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/template.xsd">

	<labels default="en">
		<locale name="en">
		<label name="forward" value="Submit"/>
		<label name="group" value="Branch"/>
		<label name="custom6.chbFdaShipper" value="Importer Consignee"/>
		<label name="custom6.chbFdaCountry" value="Address"/>
		<label name="custom6.chbFdaManid" value="City State Zip"/>
		<label name="custom4.contactName" value="Person at Importer"/>
		<label name="custom4.brokerName" value="Broker Legal Name"/>
		</locale>
	</labels>

	<section name="doc" collapsible="false" collapsed="false">
		<properties>
			<textArea row="1" col="1-2" name="voucherComments" readonly="true" styleClass="comment"/> 
			<textArea row="2" col="1-2" name="tbdComment1" styleClass="comment5"/> 
			<valueAlias row="3" col="1" name="processInformation.queueName" readonly="true"/>
		</properties>
		<actions>
			<button name="forward" command="forward" readonly="false" docType="upload" type="submit" isAnchored="false" title="forwardTitle" styleClass="button70"/>
		</actions>		
	</section>	
	
	<section name="requestPOA" visible="true" collapsible="false" collapsed="false">
		<properties>
			<dropDown row="1" col="1" name="processCode" choiceList="choice.masterfileMiscTypes" readonly="false" validation="required" onChange="reloadPage(this)"/> <!--Request Type-->
			<text row="1" col="2" name="custom4.contactNo" validation="required, noSpecialCharacters"/> <!--Contact No-->
			<dynamicDropDown row="1" col="3" name="group" providerClass="com.miriasystems.ui.providers.GroupsChoiceListProvider" validation="required"/> <!--Control Station-->
			<text row="2" col="1-3" name="chbLabel1" readonly="true" defaultValue="Please Send a POA on Behalf of:" forceDefault="true" styleClass="comment"/> 
			<text row="3" col="1" name="custom4.chbAccountNo" validation="required, noSpecialCharacters" styleClass="comment"/> <!--Account code-->
			<text row="4" col="1-3" name="custom6.chbFdaShipper" styleClass="threesixty"/> <!--Importer Consignee-->
			<text row="5" col="1-3" name="custom6.chbFdaCountry" styleClass="threesixty"/> <!--Address-->
			<text row="6" col="1-3" name="custom6.chbFdaManid" styleClass="threesixty"/> <!--City State-->
			<text row="7" col="1-3" name="custom4.contactName" styleClass="twotwenty"/> <!--Person At Importer-->
			<text row="8" col="1-3" name="chbLabel2" readonly="true" defaultValue="Issue POA To:" forceDefault="true" styleClass="comment"/> 
			<text row="9" col="1-3" name="custom4.brokerName" styleClass="threesixty"/> <!--Broker Name-->
			<text row="10" col="1-3" name="custom6.chbBrkAddress1" styleClass="twosixty"/> <!--Address-->
			<text row="11" col="1-3" name="custom6.chbBrkAddress2" styleClass="twosixty"/> <!--City State-->
			<text row="12" col="1-3" name="custom6.chbBrkFax" styleClass="onefifty"/> <!--Fax #-->
			<text row="13" col="1-3" name="custom6.chbBrkTel" styleClass="onefifty"/> <!--Tel #-->
			<text row="14" col="1-3" name="custom6.chbBrkEmail" styleClass="threesixty"/> <!--E-Mail-->
			<text row="15" col="1-3" name="custom6.chbDistrict" styleClass="fivehundred" validation="required"/> <!--District-->
			<textArea row="16" col="1-3" name="chbLabel3" readonly="true" defaultValue="Port Code Necessary if restricted POA required. POA Duration is the length of time for sub-agent POA if greater or less than 6 months (1 year max for key accounts)" forceDefault="true" styleClass="comment"/> 
			<text row="17" col="1-3" name="custom6.chbPortcode" /> <!--Port Code-->
			<text row="18" col="1" name="custom6.chbPoaReq"/> <!--POA Req-->
			<text row="19" col="1-3" name="custom6.chbPoaTime"/> <!--POA Time-->
		</properties>
		<actions>
			<iconAction name="selectApprovers" icon="/images/ico-route-w24.png" width="24" height="24" readOnlyLink="javascript:openViewRoutingWindow('$contextPath$', '$wobNumber$', 'nonPo')" link="javascript:openUploadRoutingWindow('/ManagedPay/custom', 'routing/upload.action','nonPo', 'MP_Approver1')" alwaysActive="true" readonly="false"/>
			<iconAction name="print" icon="/images/ico-print-w24.png" width="24" height="24" link="javascript:window.print()" visible="true" alwaysActive="true"/>
			<iconAction name="statusWindow" icon="/images/ico-history-w24.png" width="24" height="24" link="javascript:openStatusWindow('$seqId$', '$contextPath$');return false;" alwaysActive="true"/>
			<iconAction name="help" icon="/images/ico-help-w24.png" width="24" height="24" link="javascript:openHelp('User_Interfaces/Working_with_Invoices.htm', '$contextPath$')" alwaysActive="true"/>
		</actions>		
	</section>

	<hiddenFields>
	<hidden name="requesterId"/>
	<hidden name="scanVendorName"/>
	<hidden name="type" defaultValue="MISC"/>
	<hidden name="docPriority" defaultValue="1"/>
	<hidden name="uploadRef"/>
	<hidden name="originatorId"/>
	<hidden name="retentionBoxId" defaultValue="upload" forceDefault="true"/>
    </hiddenFields>

	<processors>
	</processors>
</propertyTemplate>
