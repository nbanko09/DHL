<?xml version="1.0" encoding="UTF-8"?>
<propertyTemplate
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/template.xsd">
		
	 <labels>
	<!-- Label overrides for this template -->
	<locale name="en">
		<label name="tbdCurrency1" value="Payment Amount"/>
		<label name="outputReference" value="Receiver Number"/>
		<label name="tbdDate2" value="Check Date"/>
 	</locale>
	</labels>
	

	<section name="paymentDetail" collapsible="false" collapsed="false">
		<properties>
			<text row="1" col="1" name="responseCode"/>
			<text row="1" col="2" name="outputReference" />
            <text row="2" col="1" name="clearingDocumentNumber" />
			<currency row="2" col="2" name="tbdCurrency1" />
            <date row="3" col="1" name="tbdDate2" />
			<currency row="3" col="2" name="clearingAmount" />
			<text row="4" col="1" name="paymentMemo" />
			<date row="4" col="2" name="clearingDate" />
		</properties>
	</section>

</propertyTemplate>
