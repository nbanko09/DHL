<?xml version="1.0" encoding="UTF-8"?>
<propertyTemplate
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/template.xsd">

	<labels default="en">
		<locale name="en">
		<label name="subType" value="Printer"/>
		<label name="processCode" value="Request Type"/>
	        <label name="createDate" value="Date Requested"/>
	        <label name="processInformation.deadline" value="Date Required"/>
	        <label name="checkStubMessage" value="Zip Code"/>
	        <label name="group" value="Control Station"/>
	        <label name="batchNumber" value="UPS #"/>
	        <label name="businessUnit" value="Payment Type"/>
			<label name="lineItems" value="Invoices To Print Checks For"/>
        </locale>
	</labels>
		
	<section name="doc" collapsible="false" collapsed="false">		
		<properties>
			<textArea row="1" col="1-2" name="tbdComment1" styleClass="comment"/> 
			<output row="2" col="1-2" name="processInformation.previousComment" readonly="true" styleClass="comment" escapeXml="false"/>
			<valueAlias row="3" col="1-2" name="processInformation.queueName" styleClass="fulllookup" readonly="true"/>
		</properties>
		<actions>
			<button name="forward" command="forward" readonly="false" docType="upload" type="submit" isAnchored="false" title="forwardTitle" styleClass="button70"/>
		</actions>
	</section>

	<section name="requestEFT" visible="true" collapsible="false" collapsed="false">
		<properties>
	        <dropDown row="1" col="1" name="processCode" choiceList="choice.checkRequestSubTypes" readonly="false" validation="required" onChange="reloadPage(this)"/> <!--Request Type-->
	        <dropDown row="1" col="2" name="businessUnit" readonly="false" validation="required" choiceList="choice.payTypes" onChange="reloadPage(this)"/> <!--Pay Type-->
	        <text row="1" col="3" name="subType" readonly="true" defaultValue="" forceDefault="true"/> <!--Print-->
	        <dynamicDropDown row="2" col="1" name="group" readonly="false" providerClass="com.miriasystems.ui.providers.GroupsChoiceListProvider" validation="required"/> <!--Control Station-->
			<dropDown row="2" col="2" name="docPriority" choiceList="choice.priorityChk" defaultValue="1" readonly="false" validation="required"/> <!--Priority-->
			<text row="3" col="1" name="checkStubMessage" readonly="true" validation="required"/> <!--station zip-->
			<text row="3" col="2" name="batchNumber" readonly="true" validation="required"/> <!--ups #-->
			<label row="4" col="1" name="" position="left"/><!--blank line-->
			<text row="5" col="1-2" name="voucherComments" readonly="true" styleClass="fulllookup" defaultValue="OVERNIGHT CHARGES TO BE PAID FOR BY REQUESTING OFFICE" forceDefault="true"/> 
		</properties>
		<actions>
			<iconAction name="selectApprovers" icon="/images/ico-route-w24.png" width="24" height="24" readOnlyLink="javascript:openViewRoutingWindow('$contextPath$', '$wobNumber$', 'nonPo')" link="javascript:openUploadRoutingWindow('/ManagedPay/custom', 'routing/upload.action','nonPo', 'MP_Approver1')" alwaysActive="true" readonly="false"/>
			<iconAction name="print" icon="/images/ico-print-w24.png" width="24" height="24" link="javascript:window.print()" visible="true" alwaysActive="true"/>
			<iconAction name="statusWindow" icon="/images/ico-history-w24.png" width="24" height="24" link="javascript:openStatusWindow('$seqId$', '$contextPath$');return false;" alwaysActive="true"/>
			<iconAction name="help" icon="/images/ico-help-w24.png" width="24" height="24" link="javascript:openHelp('User_Interfaces/Working_with_Invoices.htm', '$contextPath$')" alwaysActive="true"/>
		</actions>
	</section>


	<section name="lineItems" visible="true" collapsible="true" collapsed="false">		
		<lineItemProperties showTotal="true" showTaxTotal="false">
			<text row="1" col="1" name="tbdString1" labelKey="Vendor Code" styleClass="ninety" readonly="false" validation="required"/> <!-- Vendor Code-->
			<text row="1" col="2" name="materialId" readonly="false" validation="required" styleClass="ninety"/> <!--Voucher #-->
			<currency row="1" col="3" name="lineTotal" readonly="false" validation="required, nonZero" styleClass="currency" /> <!--Amount-->
			<dropDown row="1" col="4" name="tbdString2" labelKey="Send To" choiceList="choice.sendTo" styleClass="ninety" defaultValue="VND" readonly="false" validation="required"/> <!-- Send To-->
			<dropDown row="1" col="5" name="tbdString3" labelKey="How" choiceList="choice.howOcean" styleClass="ninety" readonly="false" validation="required"/> <!--How-->
			<text row="2" col="1-2" name="itemDescription" readonly="false" styleClass="fulllookup" validation="required"/> <!--Vendor Name-->
			<text row="2" col="3-4" name="tbdString4" labelKey="Vendor Full Address" styleClass="fulllookup" readonly="false" validation="required"/> <!-- Vendor Address-->
			<dropDown row="2" col="5" name="uom" styleClass="ninety" labelKey="Seperate EFT" readonly="false" choiceList="choice.yesNo" validation="required"/> <!-- Separate Check-->
			<lineItemHidden row="0" col="0" name="accountERPId"/>
			<lineItemHidden row="0" col="0" name="companyCode"/>
			<lineItemHidden row="0" col="0" name="costObjectTypeId1"/>
			<lineItemHidden row="0" col="0" name="costObjectTypeId2"/>
		</lineItemProperties>
		<actions>
				<!-- The following set of actions are standard ActivePay and should be included relatively unmodified from the baseline version in most templates -->
			<iconAction name="addLineItem" icon="/images/ico-add-line-w24.png" docType="upload" width="24" height="24" link="javascript:submitCommandUpload(this,'addLineItem','anchor_lineItems')" visible="true" alwaysActive="false"/>
			<iconAction name="removeLineItem" icon="/images/ico-del-line-w24.png" docType="upload" width="24" height="24" link="javascript:submitCommandUpload(this,'deleteLineItem','anchor_lineItems')" visible="true" alwaysActive="false"/>

		</actions>
	</section>

	<hiddenFields>
		<hidden name="requesterId"/>
		<hidden name="postingERPId" defaultValue="ERP1"/>
		<hidden name="vendorERPId" defaultValue="ERP1"/>
        <hidden name="newBatchNumber"/>
        <hidden name="paymentTermERPId" defaultValue="ERP1"/>
		<hidden name="type" defaultValue="CHECKREQ"/>
		<hidden name="retentionBoxId" defaultValue="upload" forceDefault="true"/>
    </hiddenFields>

	<processors>
		<processor name="com.miriasystems.processors.TotalLineAmountProcessor"/>	
    </processors>
    	
</propertyTemplate>
