<?xml version="1.0" encoding="UTF-8"?>
<view name="exp-postingfailed-view" template="pexp-tpl.xml"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd">

   
	<labels default="en">
		<locale name="en">
		</locale>
	</labels>

	
	<property-overrides>
		
	</property-overrides>

	
	<action-overrides>
		<iconAction name="editRouting" readonly="true" visible="false"/>
		<button name="update" readonly="true" visible="false"/>
		<getNext name="next" readonly="true" visible="false"/>
	</action-overrides>

	<hidden-overrides>
		<hidden name="processInformation.response" defaultValue="Complete"/>
	</hidden-overrides>

	<documentRejection>
		<rejectionReasons>
		<reason name="Reject" commentsRequired="true" note="message.terminate">
		</reason>
		</rejectionReasons>
	</documentRejection>

	<validations>
	</validations>
</view>
