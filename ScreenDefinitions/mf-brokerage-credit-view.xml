<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="masterfile-brokerage-credit-tpl.xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en">
         	<label name="forward" value="Complete"/>
        </locale>
    </labels>
    
    <property-overrides>
        	<textArea name="voucherComments" defaultValue=""/>
	        <textArea name="tbdComment1" readonly="true"/>
	        <text name = "subType" readonly="true" />
	        <text name = "group" readonly="true" />
	        <text name = "businessUnit" readonly="true" styleClass="fulllookup"/>
	        <text name = "custom4.contactNo" readonly="true" />
	        <text name = "custom4.accountNumber" readonly="true" />
        <text name = "custom4.name" readonly="true" />
     </property-overrides>

    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>

     <validations>
                   <validation name="validation.stationProductvalidation"/>   
    </validations>
    
    <documentRejection>
	  <rejectionReasons>
	  <reason name="Send to Customer Team" commentsRequired="true" note="message.customerTeam">
	  </reason>
	  <reason name="Send to Credit Team" commentsRequired="true" note="message.creditTeam">
	  </reason>
	  <reason name="Return to Requester" commentsRequired="true" note="message.requester">
	  </reason>
	   </rejectionReasons>
    </documentRejection>

</view>
