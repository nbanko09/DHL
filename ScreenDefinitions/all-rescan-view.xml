<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="all-rescan-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en"> 
        </locale>
    </labels>
	
	<property-overrides>
		
	</property-overrides>

    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>

    <documentRejection>
        <rejectionReasons>
            <reason name="Reject" commentsRequired="true" note="message.returnException">
            </reason>
			<reason name="Terminate" commentsRequired="true" note="message.terminate">
			</reason>
        </rejectionReasons>
    </documentRejection>

</view>
