<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="credit-request-upload-chb-tpl.xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en">
            <label name="forward" value="Submit"/>
            <label name="reject" value="Cancel"/>
        </locale>
    </labels>
    
    <property-overrides>
        <text name="tbdDate1" validation="required"/>
        <text name="shippingDocumentId" validation="required"/>
        <text name="number" validation="required"/>
        <text name="custom5.name" validation="required" readonly="false"/>
        <text name="custom5.accountNumber" readonly="false" validation="required"/>
    </property-overrides>
    
    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>
    
    <action-overrides>
        <button name="release" readonly="true"/>
        <getNext name="next" readonly="true" visible="false"/>
    </action-overrides>

     <documentRejection>
        <rejectionReasons>
            <reason name="Cancel" commentsRequired="true"  note="message.terminate">
            </reason>
        </rejectionReasons>
    </documentRejection>

<validations>
</validations>
</view>
