<?xml version="1.0" encoding="UTF-8"?>
<propertyTemplate
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/template.xsd">

	<labels default="en">
		<locale name="en">
		<label name="subType" value="Vendor"/>
		<label name="processCode" value="Request Type"/>
	        <label name="voucherComments" value="Message"/>
	        <label name="tbdDate1" value="Paid Thru Date"/>
	        <label name="shippingDocumentId" value="Terminal Location"/>
	        <label name="number" value="Reference #"/>
	        <label name="group" value="Control Station"/>
	        <label name="custom5.name" value="Other Vendor Name"/>
	        <label name="routingCode" value="Payment Type"/>
	        <label name="custom5.accountNumber" value="Vendor Code"/>
        </locale>
	</labels>

	<section name="doc" collapsible="false" collapsed="false">		
		<properties>
			<textArea row="1" col="1-2" name="tbdComment1" styleClass="comment"/> 
			<output row="2" col="1-2" name="processInformation.previousComment" readonly="true" styleClass="comment" escapeXml="false"/>
			<valueAlias row="3" col="1-2" name="processInformation.queueName" styleClass="fulllookup" readonly="true"/>
		</properties>
			<actions>
					<button name="forward" command="forward" readonly="false" docType="upload" type="submit" isAnchored="false" title="forwardTitle" styleClass="button70"/>
			</actions>
	</section>	
	
	<section name="Ocean Credit Card Payment" visible="true" collapsible="false" collapsed="false">
			<properties>
	                <dropDown row="1" col="1" name="processCode" choiceList="choice.crdCrdRequestSubTypes" readonly="false" validation="required" onChange="reloadPage(this)"/> <!--Request Type-->
	            	<dropDown row="1" col="2" name="subType" readonly="false" validation="required" choiceList="choice.oceanVendors" onChange="reloadPage(this)"/> <!--Vendor-->
	            	<dynamicDropDown row="1" col="3" name="group" readonly="false" providerClass="com.miriasystems.ui.providers.GroupsChoiceListProvider" validation="required"/> <!--Control Station-->
	            	<date row="2" col="1" name="tbdDate1" styleClass="date" readonly="false"/> <!--Paid Thru Date-->
	            	<text row="2" col="2" name="shippingDocumentId" readonly="false"/> <!--Terminal Location-->
	            	<text row="2" col="3" name="number" readonly="false"/> <!--Reference #-->
	              	<dropDown row="3" col="1" name="routingCode" choiceList="choice.chbPayType" readonly="true"/> <!--Payment Type #-->
	              	<text row="3" col="2" name="custom5.accountNumber" styleClass="twohundred" readonly="true"/> <!--Other Vendor #-->
	              	<text row="3" col="3" name="custom5.name" styleClass="threehundred" readonly="true"/> <!--Other Vendor #-->
			<label row="4" col="1" name="" position="left"/><!--blank line-->
			<text row="5" col="1-3" name="voucherComments" styleClass="fulllookup" defaultValue="THIS IS FOR STORAGE, EXAM OR LIFT PAYMENTS ONLY." forceDefault="true" readonly="true"/>
			</properties>

	</section>
	
		
	<section name="invoiceDetails" visible="true" collapsible="false" collapsed="false">
			<lineItemProperties showTotal="true" showTaxTotal="false">
			<text row="1" col="1" name="partNumber" labelKey="Container Number" readonly="false" styleClass="110" validation="required"/> <!-- Container Number-->
			<currency row="1" col="2" name="lineTotal" labelKey="Amount Per Cntr" readonly="false" validation="required, nonZero" styleClass="currency" onBlur="reloadPage(this)"/> <!--Amount-->
			<text row="1" col="3" name="manfPartNumber" labelKey="Voucher Number" readonly="false" styleClass="110" validation="required"/> <!-- Vocher Number-->
			<currency row="1" col="4" name="tbdCurrency1" labelKey="Voucher Amount" readonly="false" validation="required" styleClass="currency"/> <!-- Voucher Amount-->
			<lineItemHidden row="0" col="0" name="accountERPId"/>
			<lineItemHidden row="0" col="0" name="companyCode"/>
			<lineItemHidden row="0" col="0" name="costObjectTypeId1"/>
			<lineItemHidden row="0" col="0" name="costObjectTypeId2"/>
			</lineItemProperties>
						<actions>
			<iconAction name="addLineItem" icon="/images/ico-add-line-w24.png" docType="upload" width="24" height="24" link="javascript:submitCommandUpload(this,'addLineItem','anchor_lineItems')" visible="true" alwaysActive="false"/>
			<iconAction name="removeLineItem" icon="/images/ico-del-line-w24.png" docType="upload" width="24" height="24" link="javascript:submitCommandUpload(this,'deleteLineItem','anchor_lineItems')" visible="true" alwaysActive="false"/>	
		</actions>
			
	</section>
	
	<processors>
		<processor name="com.miriasystems.processors.TotalLineAmountProcessor"/>	
    </processors>
	
			

	<hiddenFields>
	<hidden name="requesterId"/>
	<hidden name="postingERPId" defaultValue="ERP1"/>
	<hidden name="vendorERPId" defaultValue="ERP1"/>
	<hidden name="newBatchNumber"/>
	<hidden name="paymentTermERPId" defaultValue="ERP1"/>
	<hidden name="type" defaultValue="CHECKREQ"/>
	<hidden name="processInformation.comment"/>
	<hidden name="uploadRef"/>
	<hidden name="originatorId"/>
	<hidden name="retentionBoxId" defaultValue="upload" forceDefault="true"/>
    </hiddenFields>

	<processors>
		<processor name="com.miriasystems.processors.TotalLineAmountProcessor"/>	
	</processors>
</propertyTemplate>
