<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="masterfile-brokerage-single-tpl.xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en">
            <label name="forward" value="Submit"/>
            <label name="reject" value="Cancel"/>
        </locale>
    </labels>
    
    <property-overrides>
    </property-overrides>

    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>
    
    <action-overrides>
        	<button name="release" visible="false"/>
   </action-overrides>
   
   <validations>
      	<validation name = "validation.checkFormat"/>
   </validations>

     <documentRejection>
		  <rejectionReasons>
			  <reason name="Cancel" commentsRequired="true" note="message.terminate">
			  </reason>
		  </rejectionReasons>
    </documentRejection>

</view>
