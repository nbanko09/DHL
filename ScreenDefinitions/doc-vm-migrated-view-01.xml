<?xml version="1.0" encoding="UTF-8"?>
<view name="doc-vm-migrated-view-01" template="doc-tax-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd">

	<labels default="en">
		<locale name="en">
		</locale>
	</labels>

	<section-overrides>	
	</section-overrides>
	
		
	<property-overrides>
		<textArea name="processInformation.comment" readonly="false"  />
		<lookup name="vendor.name" readonly="false" />
	</property-overrides>

	<lineItem-overrides>	 
	</lineItem-overrides>

	<action-overrides>
			<getNext name="next" readonly="true" visible="false" />
			<rejectButton name="reject" readonly="false"  />
			<button name="update" readonly="true" />
			<button name="forward" readonly="false" />
	</action-overrides>

	<hidden-overrides>
		<hidden name="processInformation.response" defaultValue="Complete"/>
	</hidden-overrides>

	 <documentRejection>
		<rejectionReasons>
			<reason name="Terminate" commentsRequired="true" note="message.terminate">
			</reason>
		</rejectionReasons>
	</documentRejection>

	<validations>
		<validation name="validation.validVendor" messageType="warning" note="message.validVendorValidation"/>
	</validations>
</view>
