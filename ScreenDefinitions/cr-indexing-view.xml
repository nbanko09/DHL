<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="check-request-tpl.xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en">
            <label name="forward" value="Submit"/>
        </locale>
    </labels>
    
    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>

     <documentRejection>
        <rejectionReasons>
            <reason name="Reject" commentsRequired="true"  note="message.terminate">
            </reason>
        </rejectionReasons>
    </documentRejection>

</view>
