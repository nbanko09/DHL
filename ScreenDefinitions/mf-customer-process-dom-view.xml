<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="masterfile-customer-process-dom-tpl.xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en">
            <label name="forward" value="Complete"/>
        </locale>
    </labels>
    
    <property-overrides>
        <text name="custom4.accountNumber" readonly="false" validation="required"/>
        <textArea name="voucherComments" defaultValue=""/>
        <text name="custom4.salesRep" readonly="true"/>    <!--Sales Rep-->
	<checkbox name="custom4.contractInEffect" readonly="true"/> <!--Contract In Effect-->
	<text name="custom4.contractNo" readonly="true"/>   <!--Contract Number-->
	<text name="custom4.linkToCorpAccount" readonly="true"/>   <!--Link to Corp Acc-->
	<text name="custom4.linkToArParent" readonly="true"/>  <!--Link to A/R Parent-->
	<text name="custom4.brokerName" readonly="true"/>  <!--Broker-->
	<text name="custom4.brokerAccount" readonly="true"/>  <!--Broker Ac-->
	<dropDown name="tbdString2" choiceList="choice.yesNo" readonly="true"/>  <!--Print POD on Dom Invoice-->
	<dropDown name="tbdString3" choiceList="choice.yesNo" readonly="true"/>  <!--Print charges-->
	
     </property-overrides>

    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>
    
    <action-overrides>
        	<getNext name="next" readonly="false" visible="true"/>
		<iconAction name="editRouting" readonly="true"/>
   </action-overrides>

     <documentRejection>
                 <rejectionReasons>
                     <reason name="Send to Credit Team" commentsRequired="true" note="message.creditTeam">
                         </reason>
                     <reason name="Return to Requester" commentsRequired="true" note="message.requester">
                         </reason>
                     </rejectionReasons>
    </documentRejection>

</view>
