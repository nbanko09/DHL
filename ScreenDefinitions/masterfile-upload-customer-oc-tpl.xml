<?xml version="1.0" encoding="UTF-8"?>
<propertyTemplate
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/template.xsd">

	<labels default="en">
		<locale name="en">
		<label name="forward" value="Submit"/>
		<label name="group" value="Control Station"/>
		</locale>
	</labels>

	
	<section name="doc" collapsible="false" collapsed="false">
		<properties>
			<textArea row="1" col="1-2" name="voucherComments" readonly="true" styleClass="comment"/> 
			<textArea row="2" col="1-2" name="tbdComment1" styleClass="comment"/> 
			<output row="3" col="1-2" name="processInformation.previousComment" readonly="true" styleClass="comment" escapeXml="false"/>
			<valueAlias row="4" col="1" name="processInformation.queueName" readonly="true"/>
		</properties>
		<actions>
			<button name="forward" command="forward" readonly="false" docType="upload" type="submit" isAnchored="false" title="forwardTitle" styleClass="button70"/>
		</actions>		
	</section>	
	
	<section name="requestCustomer" visible="true" collapsible="false" collapsed="false">
		<properties>
			<dropDown row="1" col="1" name="processCode" readonly="false" validation="required" choiceList="choice.product" onChange="reloadPage(this)"/><!--Product-->
			<dropDown row="1" col="2" name="subType" choiceList="choice.masterfileCustomerOcSubTypes" readonly="false" validation="required" onChange="reloadPage(this)"/> <!--Request Type-->
			<dropDown row="1" col="3" name="routingCode" choiceList="choice.customerAccountTypeOcBill" readonly="false" validation="required" onChange="reloadPage(this)"/> <!--Account Type-->
			<dropDown row="2" col="1" name="docPriority" choiceList="choice.priority" defaultValue="1" readonly="false" validation="required"/> <!--Priority-->
			<text row="2" col="2" name="custom4.contactNo" validation="required, noSpecialCharacters"/> <!--Contact No-->
			<dynamicDropDown row="2" col="3" name="group" validation="required" readonly="false" providerClass="com.miriasystems.ui.providers.GroupsChoiceListProvider"/> <!--Control Station-->
		</properties>
		<actions>
			<iconAction name="print" icon="/images/ico-print-w24.png" width="24" height="24" link="javascript:window.print()" visible="true" alwaysActive="true"/>
			<iconAction name="statusWindow" icon="/images/ico-history-w24.png" width="24" height="24" link="javascript:openStatusWindow('$seqId$', '$contextPath$');return false;" alwaysActive="true"/>
			<iconAction name="help" icon="/images/ico-help-w24.png" width="24" height="24" link="javascript:openHelp('User_Interfaces/Working_with_Invoices.htm', '$contextPath$')" alwaysActive="true"/>
		</actions>		
		</section>
		
	<section name="customerDetails" visible="true" collapsible="false" collapsed="false">
		<properties>
			<text row ="1" col="1" name="custom4.accountNumber" readonly="true" validation="required"/> <!--Account Code-->
			<text row="2" col="1-5" name="custom4.name" readonly="false" styleClass="fulllookup"/> <!--Account Name-->
			<text row="3" col="1-4" name="custom4.fao" readonly="false" styleClass="fulllookup"/> <!--Attention-->
			<text row="4" col="1-4" name="custom4.address1" readonly="false" styleClass="fulllookup"/> <!--Address 1-->
			<text row="5" col="1-4" name="custom4.address2" readonly="false" styleClass="fulllookup"/> <!--Address 2-->
			<text row="5" col="5" name="custom4.ein" readonly="false"/> <!--Tax Id-->
			<text row="6" col="1" name="custom4.city" readonly="false"/> <!--City-->
			<text row="6" col="5" name="custom4.contactName" readonly="false"/> <!--Contact Name-->
			<dropDown row="7" col="1" name="custom4.state" choiceList="choice.states" readonly="false"/> <!--State-->
			<text row="7" col="5" name="custom4.phone" readonly="false"/> <!--Phone-->
			<text row="8" col="1" name="custom4.postalCode" readonly="false"/> <!--Post Code-->
			<text row="8" col="5" name="custom4.fax" readonly="false"/> <!--Fax-->	
			<text row="9" col="1" name="custom4.country" readonly="false" defaultValue="USA"/> <!--Country-->
			<text row="9" col="3-5" name="custom4.contactEmail" styleClass="fulllookup"/> <!--E-mail-->
		</properties>
	</section>

	
	<section name="shipperInfo" visible="true" collapsible="false" collapsed="false">
		<properties>
			<checkbox row="1" col="1" name="custom4.poaReceived" readonly="false"/> <!--POA Received-->
			<checkbox row="1" col="2" name="custom4.untilRevoked" readonly="false"/> <!--Until Revoked-->
			<date row="1" col="3" name="custom4.poaExpiryDate" readonly="false"/> <!--POA Expiry Date-->
			<text row="2" col="1" name="custom4.poaLocation" readonly="false" /> <!--POA Location-->
			<dropDown row="2" col="2" name="custom4.poaFl" choiceList="choice.poaFL" readonly="false" /> <!--POA F/L-->
			<checkbox row="2" col="3" name="custom4.option4" readonly="false"/> <!--Option 4-->
			<checkbox row="3" col="1" name="custom4.selfFilling" readonly="false"/> <!--Self Filling-->
	    </properties>
	</section>

	<section name="additionalInformation" visible="true" collapsible="false" collapsed="false">
		<properties>
			<text row="1" col="1" name="custom4.accountingContact" readonly="false"/>  <!--Contact Name-->
			<text row="1" col="2" name="custom4.contactPhone" readonly="false"/> <!--Contact Tel #-->
			<text row="1" col="3" name="custom4.salesRep" readonly="false"/>    <!--Sales Rep-->
			<checkbox row="2" col="1" name="custom4.contractInEffect" readonly="false"/> <!--Contract In Effect-->
			<text row="2" col="2" name="custom4.contractNo" readonly="false"/>   <!--Contract Number-->
			<text row="2" col="3" name="custom4.linkToCorpAccount" readonly="false"/>   <!--Link to Corp Acc-->
			<text row="3" col="1" name="custom4.brokerName" readonly="false"/>  <!--Broker-->
			<text row="3" col="2" name="custom4.brokerAccount" readonly="false"/>  <!--Broker Ac-->
			<text row="4" col="1" name="custom4.scac" readonly="false"/>  <!--SCAC-->
			<checkbox row="4" col="2" name="custom4.nvocc" readonly="false"/>  <!--NVOCC-->
			<checkbox row="4" col="3" name="custom4.oceanCarrier" readonly="false"/>  <!--Ocean Carrier-->
		</properties>
	</section>


	<hiddenFields>
	<hidden name="requesterId"/>
	<hidden name="scanVendorName"/>
	<hidden name="type" defaultValue="CUSTOMER"/>
	<hidden name="tbdNumber4" defaultValue="1.0"/>
	<hidden name="uploadRef"/>
	<hidden name="originatorId"/>
	<hidden name="retentionBoxId" defaultValue="upload" forceDefault="true"/>
    </hiddenFields>
    	
	<processors>
	</processors>
</propertyTemplate>
