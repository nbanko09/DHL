<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="masterfile-upload-customer-oc1-tpl.xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en">
        </locale>
    </labels>
    
    <property-overrides>
		<textArea name="voucherComments" defaultValue="Account will be set to CASH ONLY until credit has been approved.  Customer may complete credit application online or credit application can be attached for review by the Credit Department."/>
		<text name="custom4.name" validation="required" styleClass="fulllookup"/> <!--Account Name-->
	    <text name="custom4.address1" validation="required"/> <!--Address 1-->
	    <text name="custom4.ein" validation="required"/> <!--Tax Id-->
	    <text name="custom4.city" validation="required"/> <!--City-->
	    <text name="custom4.contactName" validation="required"/> <!--Contact Name-->
	    <dropDown name="custom4.state" validation="required"/> <!--State-->
	    <text name="custom4.phone" valation="required"/> <!--Phone-->
	    <text name="custom4.postalCode" validation="required"/> <!--Post Code-->
	    <text name="custom4.country" validation="required"/> <!--Country-->
	    <dynamicDropDown name="group" validation="required"/> <!--Control Station-->
    </property-overrides>

    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>

     <validations>
                   <validation name="validation.stationProductvalidation"/>   
    </validations>

</view>
