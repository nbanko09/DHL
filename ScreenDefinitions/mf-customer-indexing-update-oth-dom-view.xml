<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="masterfile-customer-update-dom-tpl.xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en">
            <label name="forward" value="Submit"/>
            <label name="reject" value="Cancel"/>
        </locale>
    </labels>
    
    <section-overrides>
	   <section name="billTo" visible="false"/>
	   <section name="additionalInformation" visible="false" />
	   <section name="shipperInfo" visible="false"/>
    </section-overrides>
    
    <property-overrides>
        <textArea name="voucherComments" defaultValue=""/>
        <textArea name="tbdComment1" validation="required"/> 
    </property-overrides>

    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>

   <documentRejection>
		 <rejectionReasons>
		 <reason name="Cancel" commentsRequired="true" note="message.terminate">
		 </reason>
		 </rejectionReasons>
    </documentRejection>
    
    <validations>
    </validations>
</view>
