<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="masterfile-customer-update-all-con-tpl.xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en">
            <label name="forward" value="Submit"/>
            <label name="reject" value="Cancel"/>
        </locale>
    </labels>

    <property-overrides>
        <textArea name="voucherComments" defaultValue="Service Center will obtain proper approvals before completing request"/>
    </property-overrides>

    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>

    <documentRejection>
	  <rejectionReasons>
	  <reason name="Cancel" commentsRequired="true" note="message.terminate">
	  </reason>
	  </rejectionReasons>
    </documentRejection>
    
    <validations>
    </validations>

</view>
