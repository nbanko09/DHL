<?xml version="1.0" encoding="UTF-8"?>
<view name="nonpo-exception-view" template="nonpo-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd">

	<labels default="en">
		<locale name="en">
		</locale>
	</labels>

	<section-overrides>	
	</section-overrides>

	<property-overrides>
		
	</property-overrides>

	<lineItem-overrides>
	</lineItem-overrides>

	<action-overrides>
		<iconAction name="addLineItem" visible="false"/>
		<iconAction name="removeLineItem" visible="false" />
		<iconAction name="RemoveAllItems" icon="/revised/images/ico-del-all-w24.png" width="24" height="24" link="javascript:submitCommand(this,'deleteAllLineItems','anchor_lineItems')" visible="false" alwaysActive="false"/>
		<iconAction name="RemoveMarkedItems" icon="/revised/images/ico-del-match-w24.png" width="24" height="24" link="javascript:submitCommand(this,'deleteMarkedLines','anchor_lineItems')" visible="false" alwaysActive="false"/>
		<iconAction name="RemoveUnMarkedItems" icon="/revised/images/ico-del-unmatched-w24.png" width="24" height="24" link="javascript:submitCommand(this,'deleteUnMarkedLines','anchor_lineItems')" visible="false" alwaysActive="false"/>
		<iconAction name="selectTemplate" icon="/revised/images/ico-gltemplate-w24.png" width="24" height="24" link="javascript:openTemplateWindow('$contextPath$')" alwaysActive="false" visible="false"/>
		<dropDown name="lineitemsdropdown" choiceList="choice.lineitemsdropdown" alwaysActive="false" onChange="javascript:submitCommandFromDropdown(this,'addLineItem','anchor_lineItems')" visible="false"/>	
	</action-overrides>

	<hidden-overrides>
		<hidden name="processInformation.response" defaultValue="Complete"/>
	</hidden-overrides>

	<documentRejection>
		<rejectionReasons>
			<reason name="Reject" commentsRequired="false" note="message.terminate">
			</reason>
			<reason name="Rescan" commentsRequired="false" note="message.rescan">
			</reason>
			<reason name="Vendor Maintenance" commentsRequired="false" note="message.returnVendorMaintenance">
			</reason>
			<reason name="Recycle" commentsRequired="true" note="message.poChange">
			</reason>
			<reason name="Reset" commentsRequired="true" note="message.reset">
			</reason>
			<reason name="Duplicate" commentsRequired="true" note="message.duplicatereivew">  </reason>			
		</rejectionReasons>
	</documentRejection>

	<validations>
		<validation name="validation.validVendor" messageType="warning" note="message.validVendorValidation"/>
		<validation name="validation.duplicateInvoiceCheck" messageType="warning" note="message.duplicateInvoiceValidation"/>
		<validation name="validation.accountCode"/>
	</validations>
</view>
