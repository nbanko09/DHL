<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="masterfile-misc-cat-tpl.xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en">
            <label name="forward" value="Submit"/>
        </locale>
    </labels>
    
    <property-overrides>
    <text name="processCode" readonly="true" />
    <text name="custom4.contactNo" readonly="true" />
    </property-overrides>

    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>
    
    <action-overrides>
		<getNext name="next" readonly="false" visible="true"/>
		<button name="release" visible="false"/>
   </action-overrides>
   
   <documentRejection>
		  <rejectionReasons>
		  <reason name="Cancel" commentsRequired="true" note="message.terminate">
		  </reason>
		  </rejectionReasons>
    </documentRejection>
   
   

<validations>
<validation name="validation.fdacheck"/>
   	
   </validations>

</view>
