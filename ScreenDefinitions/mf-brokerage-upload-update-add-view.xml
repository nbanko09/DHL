<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="masterfile-upload-brokerage-single-tpl.xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en">
            <label name="forward" value="Submit"/>
        </locale>
    </labels>
    
    <property-overrides>
    	<text name="custom4.chbPaFao" readonly="true" />
    	<text name="custom4.chbPaAddress1" readonly="true" />
    	<text name="custom4.chbPaAddress2" readonly="true" />
    	<text name="custom4.chbPaCity" readonly="true" />
    	<text name="custom4.chbPaState" readonly="true" />
    	<text name="custom4.chbPaZip" readonly="true" />
    	<text name="custom4.chbPaCountry" readonly="true" />
    </property-overrides>

    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>
    
    <action-overrides>
        	<getNext name="next" readonly="false" visible="true"/>
   </action-overrides>

<validations>
   	<validation name = "validation.checkFormat"/>
   </validations>
</view>
