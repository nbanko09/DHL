<?xml version="1.0" encoding="UTF-8"?>
<view name="doc-rescan-view-01" template="doc-tax-tpl.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://managedpay.net/schemas/view.xsd">

	<labels default="en">
		<locale name="en">
		</locale>
	</labels>

	<section-overrides>	
	</section-overrides>
	
		
	<property-overrides>
			
			<textArea name="processInformation.comment" readonly="true"  />
			<dropDown name="subType" readonly="true" />
			<dropDown name="sensitivity" readonly="true" />
			<date name="date" readonly="true" />
			<checkbox name="tbdBoolean1" readonly="true" />
			<date name="tbdDate1" readonly="true"/>
			<dropDown name="tbdString1" readonly="true" validation=""  styleClass="onefifty" />
			<date name="tbdDate2" readonly="true" />
			<textArea name="voucherComments" readonly="true" />
			<text name="vendor.name" validation="" readonly="true"/>		
	</property-overrides>

	<lineItem-overrides>
	</lineItem-overrides>

	<action-overrides>
			<getNext name="next" readonly="true" visible="false" />
			<rejectButton name="reject" readonly="false"  />
			<button name="update" readonly="true" />
			<button name="forward" readonly="false" />
	</action-overrides>

	<hidden-overrides>
		<hidden name="processInformation.response" defaultValue="Complete"/>
	</hidden-overrides>

	 <documentRejection>
        <rejectionReasons>
            <reason name="Reject" commentsRequired="true" note="message.returnException">
            </reason>
			<reason name="Terminate" commentsRequired="true" note="message.terminate">
			</reason>
        </rejectionReasons>
	</documentRejection>

	<validations>
		<validation name="validation.validVendor" messageType="warning" note="message.validVendorValidation"/>
	</validations>
</view>
