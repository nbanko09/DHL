<?xml version="1.0" encoding="UTF-8"?>
<propertyTemplate
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/template.xsd">

	<labels default="en">
		<locale name="en">
		<label name="forward" value="Submit"/>
		<label name="group" value="Control Station"/>
		</locale>
	</labels>
	
	<section name="doc" collapsible="false" collapsed="false">
		<properties>
			<textArea row="1" col="1-2" name="voucherComments" readonly="true" styleClass="comment"/> 
			<textArea row="2" col="1-2" name="tbdComment1" styleClass="comment"/> 
			<output row="3" col="1-2" name="processInformation.previousComment" readonly="true" styleClass="comment" escapeXml="false"/>
			<valueAlias row="4" col="1" name="processInformation.queueName" readonly="true"/>
		</properties>
		<actions>
			<button name="forward" command="forward" readonly="false" docType="upload" type="submit" isAnchored="false" title="forwardTitle" styleClass="button70"/>
		</actions>
	</section>	
	
<section name="requestCustomer" visible="true" collapsible="false" collapsed="false">
		<properties>
			<dropDown row="1" col="1" name="processCode" readonly="false" validation="required" choiceList="choice.product" onChange="reloadPage(this)"/> <!--Product-->
			<dropDown row="1" col="2" name="subType" choiceList="choice.masterfileCustomerAirSubTypes" readonly="false" validation="required" onChange="reloadPage(this)"/> <!--Request Type-->
			<dropDown row="1" col="3" name="routingCode" choiceList="choice.customerAccountTypeAir" readonly="false" validation="required" onChange="reloadPage(this)"/> <!--Account Type-->
			<dropDown row="2" col="1" name="docPriority" choiceList="choice.priority" defaultValue="1" readonly="false" validation="required"/> <!--Priority-->
			<text row="2" col="2" name="custom4.contactNo" validation="required, noSpecialCharacters"/> <!--Contact No-->
			<dynamicDropDown row="2" col="3" name="group" validation="required" readonly="false" providerClass="com.miriasystems.ui.providers.GroupsChoiceListProvider"/> <!--Control Station-->
		</properties>
		<actions>
			<iconAction name="print" icon="/images/ico-print-w24.png" width="24" height="24" link="javascript:window.print()" visible="true" alwaysActive="true"/>
			<iconAction name="statusWindow" icon="/images/ico-history-w24.png" width="24" height="24" link="javascript:openStatusWindow('$seqId$', '$contextPath$');return false;" alwaysActive="true"/>
			<iconAction name="help" icon="/images/ico-help-w24.png" width="24" height="24" link="javascript:openHelp('User_Interfaces/Working_with_Invoices.htm', '$contextPath$')" alwaysActive="true"/>
		</actions>		
</section>
		
	<section name="customerDetails" visible="true" collapsible="false" collapsed="false">
		<properties>
			<text row ="1" col="1" name="custom4.accountNumber" readonly="true" validation="required"/> <!--Account Code-->
			<text row="2" col="1-5" name="custom4.name" readonly="false" validation="required" styleClass="fulllookup"/> <!--Account Name-->
			<text row="3" col="1-4" name="custom4.fao" readonly="false" styleClass="fulllookup"/> <!--Attention-->
			<text row="4" col="1-4" name="custom4.address1" readonly="false" styleClass="fulllookup" validation="required"/> <!--Address 1-->
			<text row="5" col="1-4" name="custom4.address2" readonly="false" styleClass="fulllookup"/> <!--Address 2-->
			<text row="5" col="5" name="custom4.ein" readonly="false" validation="required"/> <!--Tax Id-->
			<text row="6" col="1" name="custom4.city" readonly="false" validation="required"/> <!--City-->
			<text row="6" col="5" name="custom4.contactName" readonly="false" validation="required"/> <!--Contact Name-->
			<dropDown row="7" col="1" name="custom4.state" choiceList="choice.states" readonly="false" validation="required"/> <!--State-->
			<text row="7" col="5" name="custom4.phone" readonly="false" validation="required"/> <!--Phone-->
			<text row="8" col="1" name="custom4.postalCode" readonly="false" validation="required"/> <!--Post Code-->
			<text row="8" col="5" name="custom4.fax" readonly="false"/> <!--Fax-->	
			<text row="9" col="1" name="custom6.chbPortcode" readonly="false"/> <!--Fax-->	
			<text row="9" col="5" name="custom6.chbCatlink" readonly="false"/> <!--Fax-->	
			<text row="10" col="1" name="custom4.country" readonly="false" defaultValue="USA" forceDefault="true" validation="required"/> <!--Country-->
		</properties>
	</section>

	
	<section name="shipperInfo" visible="true" collapsible="false" collapsed="false">
		<properties>
			<checkbox row="1" col="1" name="custom4.poaReceived" readonly="false"/>
			<checkbox row="1" col="2" name="custom4.untilRevoked" readonly="false"/>
			<date row="2" col="1" name="custom4.poaExpiryDate" readonly="false"/> 
			<text row="2" col="2" name="custom4.poaLocation" readonly="false" /> 
			<dropDown row="2" col="3" name="custom4.poaFl" choiceList="choice.poaFL" readonly="false" /> 
			<checkbox row="3" col="1" name="custom4.option4" readonly="false"/>
			<checkbox row="3" col="2" name="custom4.selfFilling" readonly="false"/> 
	    </properties>
	</section>



	<section name="additionalInformation" visible="true" collapsible="false" collapsed="false">
		<properties>
			<text row="1" col="1" name="custom4.accountingContact" readonly="false"/>  
			<text row="1" col="2" name="custom4.salesRep" readonly="false"/>  			
			<text row="2" col="1" name="custom4.contactPhone" readonly="false"/> 
			<text row="2" col="2" name="custom4.contactEmail" readonly="false"/> 
			<checkbox row="3" col="1" name="custom4.contractInEffect" readonly="false"/> 
			<text row="3" col="2" name="custom4.contractNo" readonly="false"/> 
			<text row="3" col="3" name="custom4.linkToCorpContract" readonly="false"/>   
			<text row="4" col="1" name="custom4.linkToCorpAccount" readonly="false"/>  
			<text row="4" col="2" name="custom4.brokerName" readonly="false"/>  
			<text row="4" col="3" name="custom4.brokerAccount" readonly="false"/>  				
		</properties>
	</section>


	<section name="lineItems" visible="false" collapsible="true" collapsed="true">
		<lineItemProperties showTotal="true" showTaxTotal="false">
			<lookup row="1" col="1" name="accountCode" lookupType="lookup.accountCode" readonly="true" inputReadonly="true" validation="required" styleClass="twohundred"/>
			<text row="1" col="2" name="account.description" readonly="true" styleClass="twohundred"/>
			<text row="1" col="3" name="itemDescription" readonly="false" styleClass="twohundred"/>
			<currency row="1" col="4" name="lineTotal" readonly="false" validation="required, nonZero" styleClass="currency" onBlur="reloadPage(this)"/>
			<checkbox row="1" col="5" name="matched" visible="false" styleClass="ten" />					 
			<lineItemHidden row="0" col="0" name="accountERPId"/>
			<lineItemHidden row="0" col="0" name="companyCode"/>
		</lineItemProperties>
		<actions>
			<iconAction name="addLineItem" icon="/images/ico-add-line-w24.png" docType="upload" width="24" height="24" link="javascript:submitCommandUpload(this,'addLineItem','anchor_lineItems')" visible="true" alwaysActive="false"/>
			<iconAction name="removeLineItem" icon="/images/ico-del-line-w24.png" docType="upload" width="24" height="24" link="javascript:submitCommandUpload(this,'deleteLineItem','anchor_lineItems')" visible="true" alwaysActive="false"/>	
		</actions>
	</section>	
	
	<hiddenFields>
	<hidden name="requesterId"/>
	<hidden name="scanVendorName"/>
	<hidden name="type" defaultValue="CUSTOMER"/>
	<hidden name="uploadRef"/>
	<hidden name="originatorId"/>
	<hidden name="retentionBoxId" defaultValue="upload" forceDefault="true"/>
    </hiddenFields>

	<processors>
	</processors>
</propertyTemplate>
