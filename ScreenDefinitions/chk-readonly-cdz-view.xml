<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="check-request-cdz-tpl.xml" readonly="true"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en">
            <label name="forward" value="Complete"/>
        </locale>
    </labels>
    
    <property-overrides>
    </property-overrides>
    
    <lineItem-overrides>
    </lineItem-overrides>
    
    <action-overrides>
		<getNext name="next" readonly="true" visible="false" />
	</action-overrides>
    
    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>

     <documentRejection>
        <rejectionReasons>
            <reason name="Reject" commentsRequired="true"  note="message.requester">
            </reason>
        </rejectionReasons>
    </documentRejection>

</view>
