<?xml version="1.0" encoding="UTF-8"?>
<view name="all-rescan-view" template="masterfile-vendor-upd-tpl.xml"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://www.managedpay.net/schemas/view.xsd">

    <labels default="en">
        <locale name="en">
            <label name="forward" value="Submit"/>
            <label name="reject" value="Cancel"/>
        </locale>
    </labels>
    
    <property-overrides>
    <textArea name="voucherComments" defaultValue="Requests must be routed to an authorized Approver with required documentation. Address on Insurance for Certificate Holder must be:DHL Global Forwarding, Cartage Services, Trade Center III, 2580 Esters BLVD, PO BOX 612648, DFW Airport, TX 75216"/>
	<text name="custom5.accountNumber" validation="required" readonly="false"/>
	<text name="custom5.accountAccountingNumber" readonly="true" />
	<text name="custom5.name" validation="required" />
	<text name="custom5.taxId" readonly="true" />
	<text name="docPriority" readonly="false"/>
    </property-overrides>
    
    
    <documentRejection>
		<rejectionReasons>
			<reason name="Cancel" commentsRequired="true" note="message.terminate">
				</reason>
		</rejectionReasons>
    </documentRejection>

	<validations>
        <validation name="validation.checkRouting"/>
        <validation name="validation.stationProductvalidation"/>                    
    </validations>

    <hidden-overrides>
        <hidden name="processInformation.response" defaultValue="Complete"/>
    </hidden-overrides>
</view>
